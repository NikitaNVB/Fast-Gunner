            <string>/// check if player cleared room

var zombiesToKill = instance_number(obj_EnemyZombie);
global.timer -= 1/room_speed;

if (global.timer &lt;= 0) {
    obj_PlayerStats.hp = 0;
}

if (!instance_exists(obj_EnemyZombie) &amp;&amp; !global.gameOver) {
    global.level++; // increase level
    obj_PlayerStats.maxHp += 10;
    if ((obj_PlayerStats.hp + 10) &lt;= obj_PlayerStats.maxHp)
        obj_PlayerStats.hp += 10;
    if (global.randLoop &gt; 0)
